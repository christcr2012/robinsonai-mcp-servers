{
  "mcpServers": {
    "architect-mcp": {
      "command": "npx",
      "args": ["architect-mcp"],
      "env": {
        "OLLAMA_BASE_URL": "http://127.0.0.1:11434",
        "ARCHITECT_FAST_MODEL": "qwen2.5:3b",
        "ARCHITECT_STD_MODEL": "deepseek-coder:33b",
        "ARCHITECT_BIG_MODEL": "qwen2.5-coder:32b",
        "ARCHITECT_MAX_STEPS": "8",
        "ARCHITECT_PLANNER_TIME_MS": "45000",
        "ARCHITECT_PLANNER_SLICE_MS": "2000"
      }
    },
    "autonomous-agent-mcp": {
      "command": "npx",
      "args": ["autonomous-agent-mcp"],
      "env": {
        "OLLAMA_BASE_URL": "http://127.0.0.1:11434",
        "MAX_OLLAMA_CONCURRENCY": "12"
      }
    },
    "credit-optimizer-mcp": {
      "command": "npx",
      "args": ["credit-optimizer-mcp"],
      "env": {}
    },
    "thinking-tools-mcp": {
      "command": "npx",
      "args": ["thinking-tools-mcp"],
      "env": {}
    },
    "openai-worker-mcp": {
      "command": "npx",
      "args": ["openai-worker-mcp"],
      "env": {
        "OPENAI_API_KEY": "<YOUR_OPENAI_API_KEY_HERE>",
        "MONTHLY_BUDGET": "25",
        "MAX_OPENAI_CONCURRENCY": "15",
        "PER_JOB_TOKEN_LIMIT": "6000"
      }
    },
    "robinsons-toolkit-mcp": {
      "command": "npx",
      "args": ["robinsons-toolkit-mcp"],
      "env": {
        "GITHUB_TOKEN": "<YOUR_GITHUB_TOKEN_HERE>",
        "VERCEL_TOKEN": "<YOUR_VERCEL_TOKEN_HERE>",
        "NEON_API_KEY": "<YOUR_NEON_API_KEY_HERE>",
        "UPSTASH_API_KEY": "<YOUR_UPSTASH_API_KEY_HERE>",
        "UPSTASH_EMAIL": "<YOUR_UPSTASH_EMAIL_HERE>",
        "UPSTASH_REDIS_REST_URL": "<YOUR_UPSTASH_REDIS_REST_URL_HERE>",
        "UPSTASH_REDIS_REST_TOKEN": "<YOUR_UPSTASH_REDIS_REST_TOKEN_HERE>"
      }
    }
  }
}

