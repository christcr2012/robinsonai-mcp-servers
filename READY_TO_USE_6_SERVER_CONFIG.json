{
  "mcpServers": {
    "architect-mcp": {
      "command": "npx",
      "args": ["architect-mcp"],
      "env": {
        "OLLAMA_BASE_URL": "http://127.0.0.1:11434",
        "ARCHITECT_FAST_MODEL": "qwen2.5:3b",
        "ARCHITECT_STD_MODEL": "deepseek-coder:33b",
        "ARCHITECT_BIG_MODEL": "qwen2.5-coder:32b",
        "ARCHITECT_MAX_STEPS": "8",
        "ARCHITECT_PLANNER_TIME_MS": "45000",
        "ARCHITECT_PLANNER_SLICE_MS": "2000"
      }
    },
    "autonomous-agent-mcp": {
      "command": "npx",
      "args": ["autonomous-agent-mcp"],
      "env": {
        "OLLAMA_BASE_URL": "http://127.0.0.1:11434",
        "MAX_OLLAMA_CONCURRENCY": "2"
      }
    },
    "credit-optimizer-mcp": {
      "command": "npx",
      "args": ["credit-optimizer-mcp"],
      "env": {}
    },
    "thinking-tools-mcp": {
      "command": "npx",
      "args": ["thinking-tools-mcp"],
      "env": {}
    },
    "openai-worker-mcp": {
      "command": "npx",
      "args": ["openai-worker-mcp"],
      "env": {
        "OPENAI_API_KEY": "sk-your-openai-api-key-here",
        "MONTHLY_BUDGET": "25",
        "MAX_OPENAI_CONCURRENCY": "1",
        "PER_JOB_TOKEN_LIMIT": "6000"
      }
    },
    "robinsons-toolkit-mcp": {
      "command": "npx",
      "args": ["robinsons-toolkit-mcp"],
      "env": {
        "RTK_EAGER_LOAD": "1",
        "RTK_MAX_ACTIVE": "12",
        "RTK_TOOL_TIMEOUT_MS": "60000",
        "GITHUB_TOKEN": "ghp_your-github-token-here",
        "VERCEL_TOKEN": "your-vercel-token-here",
        "NEON_API_KEY": "your-neon-api-key-here",
        "NEON_DATABASE_URL": "postgresql://user:password@host/database",
        "STRIPE_SECRET_KEY": "sk_test_your-stripe-key-here",
        "STRIPE_API_KEY": "sk_test_your-stripe-key-here",
        "SUPABASE_URL": "https://your-project.supabase.co",
        "SUPABASE_ANON_KEY": "your-supabase-anon-key-here",
        "SUPABASE_SERVICE_KEY": "your-supabase-service-key-here",
        "TWILIO_ACCOUNT_SID": "ACyour-twilio-account-sid-here",
        "TWILIO_AUTH_TOKEN": "your-twilio-auth-token-here",
        "RESEND_API_KEY": "re_your-resend-api-key-here",
        "RESEND_FROM": "your-email@yourdomain.com",
        "CLOUDFLARE_API_TOKEN": "your-cloudflare-api-token-here",
        "REDIS_URL": "redis://default:password@host:port",
        "REDIS_CLOUD_API_KEY": "your-redis-cloud-api-key-here",
        "REDIS_CLOUD_API_SECRET": "your-redis-cloud-api-secret-here",
        "FLY_API_TOKEN": "your-fly-api-token-here",
        "GOOGLE_SERVICE_ACCOUNT_KEY": "path/to/your/service-account.json",
        "GOOGLE_CREDENTIALS_JSON": "path/to/your/service-account.json",
        "GOOGLE_USER_EMAIL": "your-email@yourdomain.com",
        "GOOGLE_IMPERSONATE_EMAIL": "your-email@yourdomain.com"
      }
    }
  }
}

