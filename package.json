{
  "name": "robinsonai-mcp-servers",
  "version": "1.0.0",
  "description": "Comprehensive MCP servers for Vercel, Neon, Redis, GitHub, and more - by Robinson AI Systems",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "build:vercel": "npm run build -w @robinsonai/vercel-mcp",
    "build:neon": "npm run build -w @robinsonai/neon-mcp",
    "build:redis": "npm run build -w @robinsonai/redis-mcp",
    "build:github": "npm run build -w @robinsonai/github-mcp",
    "link:all": "npm run link --workspaces --if-present",
    "qa": "node scripts/qa.mjs",
    "accept": "node scripts/accept.mjs",
    "precommit": "npm run qa"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "vercel",
    "neon",
    "redis",
    "github",
    "ai",
    "automation"
  ],
  "author": "Robinson AI Systems",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "openai": "^6.5.0"
  }
}
