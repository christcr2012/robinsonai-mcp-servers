{
  "name": "@robinson_ai_systems/agent-core",
  "version": "0.0.0-internal",
  "description": "Internal core library for Agent MCPs (bundled, not published)",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./generation/*": "./dist/generation/*.js",
    "./patterns/*": "./dist/patterns/*.js",
    "./repo/*": "./dist/repo/*.js",
    "./utils/*": "./dist/utils/*.js",
    "./anchors/*": "./dist/anchors/*.js",
    "./patch/*": "./dist/patch/*.js",
    "./spec/*": "./dist/spec/*.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run"
  },
  "dependencies": {
    "diff": "^5.2.0",
    "fast-glob": "^3.3.2",
    "pg": "^8.13.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/pg": "^8.11.10",
    "typescript": "^5.7.2",
    "tsup": "^8.5.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
