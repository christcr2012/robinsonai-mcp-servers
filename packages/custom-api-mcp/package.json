{
  "name": "@robinson_ai_systems/custom-api-mcp",
  "version": "0.1.0",
  "description": "Custom API MCP - Connect to Chris's FastAPI server (PostgreSQL, Neo4j, Qdrant, N8N)",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "custom-api-mcp": "./bin/custom-api-mcp.js"
  },
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "bin/"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "test": "echo \"Tests coming soon\"",
    "prepack": "npm run clean && npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "fastapi",
    "postgresql",
    "neo4j",
    "qdrant",
    "n8n"
  ],
  "author": "Robinson AI Systems",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@robinson_ai_systems/shared-llm": "^0.1.12",
    "@robinson_ai_systems/shared-pipeline": "^0.1.1",
    "@robinson_ai_systems/shared-utils": "^0.1.1",
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/robinsonai/robinsonai-mcp-servers.git",
    "directory": "packages/custom-api-mcp"
  }
}

