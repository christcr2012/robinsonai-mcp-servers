{
  "name": "@robinson_ai_systems/robinsons-context-engine",
  "version": "1.1.2",
  "description": "Production-grade context engine with hybrid search, multiple embedding providers, and intelligent model selection",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./embeddings": {
      "types": "./dist/embeddings.d.ts",
      "import": "./dist/embeddings.js"
    },
    "./store": {
      "types": "./dist/store.d.ts",
      "import": "./dist/store.js"
    },
    "./symbol-aware": {
      "types": "./dist/symbol-aware.d.ts",
      "import": "./dist/symbol-aware.js"
    },
    "./import-graph": {
      "types": "./dist/import-graph.d.ts",
      "import": "./dist/import-graph.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "context-engine",
    "embeddings",
    "vector-search",
    "hybrid-search",
    "openai",
    "claude",
    "ollama",
    "code-search"
  ],
  "author": "Robinson AI Systems",
  "license": "MIT",
  "dependencies": {
    "@robinson_ai_systems/free-agent-mcp": "^0.1.29",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3"
  }
}
