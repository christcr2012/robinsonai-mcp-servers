{
  "name": "@robinson_ai_systems/robinsons-context-engine",
  "version": "1.2.0",
  "description": "Robinson's Context Engine - Production-grade hybrid search with vector similarity, language-aware ranking, and intelligent file filtering",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc -b ../../tsconfig.json --force",
    "prepack": "npm run clean && npm run build"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "keywords": [
    "context-engine",
    "embeddings",
    "vector-search",
    "hybrid-search",
    "openai",
    "claude",
    "ollama",
    "code-search"
  ],
  "author": "Robinson AI Systems",
  "license": "MIT",
  "dependencies": {
    "@robinson_ai_systems/shared-utils": "workspace:*"
  },
  "devDependencies": {
    "rimraf": "^6.0.0",
    "typescript": "^5.6.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
