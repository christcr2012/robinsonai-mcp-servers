{
  "name": "@robinson_ai_systems/free-agent-core",
  "version": "1.0.16",
  "description": "Core library for Free Agent MCP - code generation with FREE models",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./generation/*": "./dist/generation/*.js",
    "./patterns/*": "./dist/patterns/*.js",
    "./repo/*": "./dist/repo/*.js",
    "./utils/*": "./dist/utils/*.js",
    "./anchors/*": "./dist/anchors/*.js",
    "./patch/*": "./dist/patch/*.js",
    "./spec/*": "./dist/spec/*.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run"
  },
  "dependencies": {
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.7.2",
    "tsup": "^8.5.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
