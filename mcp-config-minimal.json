{
  "mcpServers": {
    "github": {
      "command": "npx",
      "args": [
        "github-mcp"
      ],
      "env": {
        "GITHUB_TOKEN": "YOUR_GITHUB_TOKEN_HERE"
      }
    },
    "vercel": {
      "command": "npx",
      "args": [
        "vercel-mcp"
      ],
      "env": {
        "VERCEL_TOKEN": "YOUR_VERCEL_TOKEN_HERE"
      }
    },
    "neon": {
      "command": "npx",
      "args": [
        "neon-mcp"
      ],
      "env": {
        "NEON_API_KEY": "YOUR_NEON_API_KEY_HERE"
      }
    }
  }
}

