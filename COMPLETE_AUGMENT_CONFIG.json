{
  "mcpServers": {
    "architect-mcp": {
      "command": "npx",
      "args": ["-y", "@robinsonai/architect-mcp"],
      "env": {
        "OLLAMA_BASE_URL": "http://localhost:11434",
        "ARCHITECT_FAST_MODEL": "qwen2.5:3b",
        "ARCHITECT_STD_MODEL": "deepseek-coder:33b",
        "ARCHITECT_BIG_MODEL": "qwen2.5-coder:32b"
      }
    },
    "autonomous-agent-mcp": {
      "command": "npx",
      "args": ["-y", "@robinsonai/autonomous-agent-mcp"],
      "env": {
        "OLLAMA_BASE_URL": "http://localhost:11434"
      }
    },
    "credit-optimizer-mcp": {
      "command": "npx",
      "args": ["-y", "@robinsonai/credit-optimizer-mcp"]
    },
    "robinsons-toolkit-mcp": {
      "command": "npx",
      "args": ["-y", "@robinsonai/robinsons-toolkit-mcp"]
    },
    "github-mcp": {
      "command": "npx",
      "args": ["-y", "@robinsonai/github-mcp"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "YOUR_GITHUB_TOKEN_HERE"
      }
    },
    "vercel-mcp": {
      "command": "npx",
      "args": ["-y", "@robinsonai/vercel-mcp"],
      "env": {
        "VERCEL_API_TOKEN": "YOUR_VERCEL_TOKEN_HERE"
      }
    },
    "neon-mcp": {
      "command": "npx",
      "args": ["-y", "@robinsonai/neon-mcp"],
      "env": {
        "NEON_API_KEY": "YOUR_NEON_API_KEY_HERE"
      }
    },
    "playwright-mcp": {
      "command": "npx",
      "args": ["-y", "@robinsonai/playwright-mcp"]
    },
    "sequential-thinking-mcp": {
      "command": "npx",
      "args": ["-y", "@robinsonai/sequential-thinking-mcp"]
    },
    "context7-mcp": {
      "command": "npx",
      "args": ["-y", "@robinsonai/context7-mcp"]
    },
    "twilio-mcp": {
      "command": "npx",
      "args": ["-y", "@robinsonai/twilio-mcp"],
      "env": {
        "TWILIO_ACCOUNT_SID": "YOUR_TWILIO_ACCOUNT_SID_HERE",
        "TWILIO_AUTH_TOKEN": "YOUR_TWILIO_AUTH_TOKEN_HERE"
      }
    },
    "resend-mcp": {
      "command": "npx",
      "args": ["-y", "@robinsonai/resend-mcp"],
      "env": {
        "RESEND_API_KEY": "YOUR_RESEND_API_KEY_HERE"
      }
    },
    "cloudflare-mcp": {
      "command": "npx",
      "args": ["-y", "@robinsonai/cloudflare-mcp"],
      "env": {
        "CLOUDFLARE_API_TOKEN": "YOUR_CLOUDFLARE_API_TOKEN_HERE"
      }
    },
    "redis-mcp": {
      "command": "npx",
      "args": ["-y", "@robinsonai/redis-mcp"],
      "env": {
        "REDIS_URL": "redis://localhost:6379"
      }
    },
    "openai-mcp": {
      "command": "npx",
      "args": ["-y", "@robinsonai/openai-mcp"],
      "env": {
        "OPENAI_API_KEY": "YOUR_OPENAI_API_KEY_HERE"
      }
    },
    "google-workspace-mcp": {
      "command": "npx",
      "args": ["-y", "@robinsonai/google-workspace-mcp"],
      "env": {
        "GOOGLE_CLIENT_ID": "YOUR_GOOGLE_CLIENT_ID_HERE",
        "GOOGLE_CLIENT_SECRET": "YOUR_GOOGLE_CLIENT_SECRET_HERE",
        "GOOGLE_REFRESH_TOKEN": "YOUR_GOOGLE_REFRESH_TOKEN_HERE"
      }
    }
  }
}

