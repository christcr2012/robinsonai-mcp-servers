{
  "mcpServers": {
    "Free Agent MCP": {
      "command": "npx",
      "args": [
        "-y",
        "@robinson_ai_systems/free-agent-mcp@0.1.6"
      ],
      "env": {
        "OLLAMA_BASE_URL": "http://localhost:11434",
        "MAX_OLLAMA_CONCURRENCY": "15",
        "DEFAULT_OLLAMA_MODEL": "qwen2.5-coder:7b",
        "OLLAMA_START_TIMEOUT": "120",
        "FAST_MODEL": "qwen2.5:3b",
        "MEDIUM_MODEL": "qwen2.5-coder:7b",
        "COMPLEX_MODEL": "qwen2.5-coder:7b"
      }
    },
    "Paid Agent MCP": {
      "command": "npx",
      "args": [
        "-y",
        "@robinson_ai_systems/paid-agent-mcp@0.2.3"
      ],
      "env": {
        "MONTHLY_BUDGET": "25",
        "MAX_OPENAI_CONCURRENCY": "15"
      }
    },
    "Thinking Tools MCP": {
      "command": "npx",
      "args": [
        "-y",
        "@robinson_ai_systems/thinking-tools-mcp"
      ]
    },
    "Credit Optimizer MCP": {
      "command": "npx",
      "args": [
        "-y",
        "@robinson_ai_systems/credit-optimizer-mcp@0.1.6"
      ],
      "env": {
        "CREDIT_OPTIMIZER_SKIP_INDEX": "0"
      }
    },
    "Robinson's Toolkit MCP": {
      "command": "npx",
      "args": [
        "-y",
        "@robinson_ai_systems/robinsons-toolkit-mcp@1.0.2"
      ],
      "env": {
        "GOOGLE_SERVICE_ACCOUNT_KEY": "C:\\Users\\chris\\.config\\google-workspace-mcp\\service-account.json",
        "GOOGLE_USER_EMAIL": "ops@robinsonaisystems.com",
        "UPSTASH_EMAIL": "ops@robinsonaisystems.com"
      }
    }
  }
}

